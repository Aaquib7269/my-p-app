<div class="col-md-6">
    <div class="box box-solid">
        <div class="box-header">
            <i class="fa fa-th"></i>
            <h3 class="box-title">Users</h3>
        </div>
        <div class="box-body border-radius-none">
            <div class="chart">
                <%= column_chart Hash[@users_data], xtitle: "Year (#{DateTime.now.year})", ytitle: "Users Registered" %>
            </div>
        </div>
    </div>
</div>
<div class="col-md-6">
    <div class="box box-solid">
        <div class="box-header">
            <i class="fa fa-th"></i>
            <h3 class="box-title">Products</h3>
        </div>
        <div class="box-body border-radius-none">
            <div class="chart">
                <%= column_chart Hash[@products_data], xtitle: "Year (#{DateTime.now.year})", ytitle: "Products Added" %>
            </div>
        </div>
    </div>
</div>
